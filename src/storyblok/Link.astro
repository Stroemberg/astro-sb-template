---
import { storyblokEditable } from '../utils/storyblokEditable';
import type { StoryblokLink } from '../../.storyblok/types/289979697450186/storyblok-components';

type Props = {
	blok: StoryblokLink;
};
const { blok } = Astro.props;
---

<a
	class="link link-primary"
	{...storyblokEditable(blok)}
	href={blok.link.cached_url}
	>{blok.text}
</a>
